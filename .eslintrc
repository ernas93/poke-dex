{
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true
  },
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "quotes": ["error", "single"],
    "prettier/prettier": ["error", {
      "singleQuote": true
    }]
  }
}